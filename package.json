{
  "name": "sweets_x_b",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run format && npm run lint && npm run test && tsc -b && vite build && node -e \"console.log('\\n\\u001b[32m✅ BUILD SUCCESSFUL\\u001b[0m\\n')\" || node -e \"console.error('\\n\\u001b[31m❌ BUILD FAILED\\u001b[0m\\n')\"",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "check": "npm run lint && npm run test",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "fresh-install": "powershell -Command \"Remove-Item -Recurse -Force node_modules; Remove-Item -Force package-lock.json; npm cache clean --force; npx npm-check-updates -u; npm install\"",
    "prepush": "npm run fresh-install && npm run build"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/aria-query": "^5.0.4",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^5.1.0",
    "ansi-regex": "^6.2.2",
    "ansi-styles": "^6.2.3",
    "aria-query": "^5.3.2",
    "dequal": "^2.0.3",
    "dom-accessibility-api": "^0.7.0",
    "eslint": "^9.39.0",
    "eslint-plugin-node-import": "^1.0.5",
    "eslint-plugin-react-dom": "^2.3.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-react-x": "^2.3.1",
    "globals": "^16.5.0",
    "jsdom": "^27.1.0",
    "lz-string": "^1.5.0",
    "picomatch": "^4.0.3",
    "prettier": "^3.6.2",
    "pretty-format": "^30.2.0",
    "react-is": "^19.2.0",
    "react-router-dom": "^7.9.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.12",
    "vitest": "^4.0.6"
  }
}
